language = {English},
pages = {223},
publisher = {Colorado State University PP  - United States -- Colorado},
title = {{‘Demand Management' and Injustice in Rural Agricultural Irrigation in Western Colorado: An Anatomy of Ambivalence}},
url = {https://www.proquest.com/dissertations-theses/demand-management-injustice-rural-agricultural/docview/2763567961/se-2?accountid=14026 http://library.stanford.edu/sfx?url{\_}ver=Z39.88-2004{\&}rft{\_}val{\_}fmt=info:ofi/fmt:kev:mtx:dissertation{\&}genre=dissertations{\&}sid=ProQ:ProQuest+Dissertations+{\%}26+Theses+Global{\&}atitle={\&}title=‘Demand+Management'+and+Injustice+in+Rural+Agricultural+Irrigation+in+Western+Colorado{\%}3A+An+Anatomy+of+Ambivalence{\&}issn={\&}date=2022-01-01{\&}volume={\&}issue={\&}spage={\&}au=MacIlroy{\%}2C+Kelsea+E.{\&}isbn=9798368428987{\&}jtitle={\&}btitle={\&}rft{\_}id=info:eric/{\&}rft{\_}id=info:doi/},
