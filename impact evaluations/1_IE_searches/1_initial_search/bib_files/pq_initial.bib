keywords = {0501:Economics,0503:Agricultural economics,Acequias,Agricultural economics,Common pool resources,Common-pool irrigation,Contingent valuation,Economics,Experimental economics,Institutional analysis,New Mexico,Public welfare values,Social sciences},
language = {English},
