@article{Abdelrahman2020,
abstract = {Modern irrigation systems are key to improving the productivity of water and land units. This study assesses the economic aspects of the drip irrigation system (DIS) by calculating the investment profitability and economic efficiency of the DIS for growing tomatoes in Egypt. One hundred tomato farmers were surveyed. Financial discounted criteria were used in addition to data envelopment analysis to estimate the economic efficiency of growing tomatoes using the DIS. The results showed that investment in the DIS is lucrative, with an increase of 67{\%} in the net return per hectare, compared with results using non‐drip irrigation. The benefit–cost ratio amounted to 1.35, the net present value was US{\$}1720, and the internal rate of return was 44{\%}. The data envelopment analysis results revealed that most farms achieved technical and allocative efficiencies; however, cost and scale efficiencies still need improvement. These results could guide policymakers when designing irrigation transformation strategies that involve the use of micro‐credit to encourage small farmers to change from conventional irrigation systems to DISs. Agricultural extension agencies should organize intensive training programmes for small farmers on how to operate and maintain the DIS. These policies will accelerate the adoption of irrigation modernization and help vulnerable farmers maximize productivity and achieve sustainability in the use of limited resources.Alternate abstract:R{\'{e}}sum{\'{e}}Les syst{\`{e}}mes d'irrigation modernes sont l'{\'{e}}l{\'{e}}ment cl{\'{e}} pour am{\'{e}}liorer la productivit{\'{e}} de l'eau et de l'unit{\'{e}} terrestre. Cette {\'{e}}tude vise {\`{a}} {\'{e}}valuer les aspects {\'{e}}conomiques du syst{\`{e}}me d'irrigation goutte {\`{a}} goutte (DIS) en estimant la rentabilit{\'{e}} des investissements et l'efficacit{\'{e}} {\'{e}}conomique du DIS pour la tomate en {\'{E}}gypte. Une centaine de producteurs de tomates ont {\'{e}}t{\'{e}} enqu{\^{e}}t{\'{e}}s. Des crit{\`{e}}res financiers actualis{\'{e}}s ont {\'{e}}t{\'{e}} utilis{\'{e}}s, en plus de l'utilisation de l'analyse d'enveloppement des donn{\'{e}}es pour estimer l'efficacit{\'{e}} {\'{e}}conomique de la tomate sous DIS. Les r{\'{e}}sultats ont r{\'{e}}v{\'{e}}l{\'{e}} que l'investissement dans le DIS est un investissement lucratif, le rendement net {\`{a}} l'hectare ayant augment{\'{e}} de 67{\%} par rapport {\`{a}} l'irrigation sans goutte {\`{a}} goutte, le rapport avantages–co{\^{u}}ts s'{\'{e}}levant {\`{a}} 1.35, la valeur actuelle nette {\`{a}} US{\$}1720 et le taux de rendement interne {\`{a}} 44{\%}. Le r{\'{e}}sultat de l'analyse d'enveloppement des donn{\'{e}}es a r{\'{e}}v{\'{e}}l{\'{e}} que la plupart des exploitations avaient atteint une efficacit{\'{e}} technique et d'allocation des ressources, mais que les {\'{e}}conomies de co{\^{u}}ts et d'{\'{e}}chelle devaient encore {\^{e}}tre am{\'{e}}lior{\'{e}}es. Ces r{\'{e}}sultats pourraient aider les d{\'{e}}cideurs politiques {\`{a}} concevoir une strat{\'{e}}gie de transformation de l'irrigation par micro‐cr{\'{e}}dit afin d'encourager les petits agriculteurs {\`{a}} passer des syst{\`{e}}mes d'irrigation classiques aux syst{\`{e}}mes DIS. Les agences de vulgarisation agricole devraient organiser des programmes de formation intensive {\`{a}} l'intention des petits agriculteurs sur la mani{\`{e}}re d'utiliser et d'entretenir le DIS. Ces politiques acc{\'{e}}l{\'{e}}reront l'adoption de la modernisation de l'irrigation et aideront les agriculteurs vuln{\'{e}}rables {\`{a}} optimiser leur productivit{\'{e}} et {\`{a}} utiliser durablement leurs ressources limit{\'{e}}es.},
address = {Agricultural Economics and Management, Huazhong Agriculture University, Wuhan, Hubei, China; Agricultural Economics, Fayoum University Faculty of Agriculture, Fayoum, Egypt ; Agricultural Economics and Management, Huazhong Agriculture University, Wuhan, Hubei, China ; Agricultural Economics and Management, Huazhong Agriculture University, Wuhan, Hubei, China; Agricultural Economics, Fayoum University Faculty of Agriculture, Fayoum, Egypt},
