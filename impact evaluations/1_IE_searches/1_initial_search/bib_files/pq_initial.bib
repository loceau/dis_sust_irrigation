@article{HALES2000185,
abstract = {The Rio Cobre Irrigation Scheme, Jamaica, supplies water to 5000ha by allocating a fixed unit discharge at each offtake. A computer water management model, IRMOS, was used to assess the scheme's performance during a typical operating year and to identify water management constraints to performance. An optimal water allocation policy was used as a benchmark against which the actual performance was evaluated. The comparison identified constraints in the scheme which can be improved by a variety of measures which include modifying the water allocation policy, operational procedures and physical components of the scheme. Despite the fact that operating under an optimal water allocation policy requires more data, better monitoring and a higher calibre of staff, the analysis shows that the potential benefits far outweigh the additional costs. The process shows how the use of modelling to identify performance shortfalls followed by management (rather than infrastructural) interventions can be a valid way forward to improving scheme performance.},
author = {Hales, A L and Burton, M A},
doi = {https://doi.org/10.1016/S0378-3774(99)00076-1},
issn = {0378-3774},
journal = {Agricultural Water Management},
keywords = { Computer model, Diagnostic analysis, Optimization, Performance,Irrigation management},
number = {2},
pages = {185--202},
title = {{Using the IRMOS model for diagnostic analysis and performance enhancement of the Rio Cobre Irrigation Scheme, Jamaica}},
url = {https://www.sciencedirect.com/science/article/pii/S0378377499000761},
volume = {45},
year = {2000}
}
@article{AHMADZADEH201615,
abstract = {Considering global water scarcity, especially in the agricultural sector, many attempts are being made to improve water productivity and save more water. Changing irrigation systems from surface to pressurized has been one of the considerable actions in this regard. However, any attempts to do so must be evaluated in an integrated perspective at the basin level by means of relevant agro-hydrological models before making them operational. This paper aims to address this issue by using the soil and water assessment tool (SWAT). Zarrineh Rud River, the main feeding river of northwest Iran's Lake Urmia, is selected as the case study for exploring this methodology. This lake has been shrinking since 1995, and to save water and increase the basin's water productivity, improvement of pressurized systems is considered as a solution. SWAT cannot directly simulate changing the irrigation system from surface to pressurized. In this study, we use the innovative approach of applying the model to simulate different irrigation systems considering real irrigation management variables such as depth and date of each irrigation event. Also, in order to improve its performance in simulation of such systems, a comprehensive calibration procedure was used based on a wide range of data for hydrological and agricultural variables. The results showed that changing the current irrigation to a pressurized system can increase water productivity up to 15{\%} due to increases in crop yield, better water distribution and greater actual evapotranspiration. However, pressurized irrigation results in no significant change in total inflow to the lake. Notably, these systems can intensify drawdown of the basin's water table up to 20{\%}. So, any significant “real water saving” program in the basin must be associated with reduction of evapotranspiration by adopting measures like reducing cultivated areas, changing cropping patterns to less water consuming plants, or applying deficit irrigation. The applied methodology of this paper as well as the comprehensive calibration and setup of the SWAT model with the readily available hydrological and agricultural variables can be a good sample for similar works. However, further work is still needed to more broadly test this approach in areas with intensive irrigation systems.},
annote = {Agricultural water and nonpoint source pollution management at a watershed scale: PART I Overseen by: Brent Clothier},
author = {Ahmadzadeh, Hojat and Morid, Saeed and Delavar, Majid and Srinivasan, Raghavan},
doi = {https://doi.org/10.1016/j.agwat.2015.10.026},
issn = {0378-3774},
journal = {Agricultural Water Management},
keywords = { Iran, Lake Urmia, Pressurized irrigation system, SWAT model, Water saving,Water productivity},
pages = {15--28},
title = {{Using the SWAT model to assess the impacts of changing irrigation from surface to pressurized systems on water productivity and water saving in the Zarrineh Rud catchment}},
url = {https://www.sciencedirect.com/science/article/pii/S0378377415301463},
volume = {175},
year = {2016}
}
@phdthesis{Pascuet2005,
abstract = {Distribution Uniformity (DU) of irrigation water is important because of its effects over crop yields, operational costs, irrigation scheduling, salinity control, and system design. Traditionally, irrigation system evaluations rarely report global distribution uniformity because they do not take into account all the factors that influence uniformity across the field. The Irrigation Training and Research Center (ITRC) developed standardized procedures for the evaluation of on-farm irrigation systems. These methods estimate the global DUlq and also determine the causes and relative importance of the factors influencing the non-uniformity. This study contrasts the traditional rapid {\"{I}}TRC method used to collect data and to compute global DUlq for a hand move sprinkler irrigation system, against a global DUlq calculated with catch-can data from throughout the whole field.Six hand move sprinkler irrigation systems were designed for a single field, assuming different slope configurations, and the pressure distribution of each one was determined. The comparison assumed a simplified case where all the flow and pattern variation comes from pressure differences, assuming no wind, plugging or wear.Indoor single leg profile test results were provided by Rain Bird for a 20JH impact sprinkler with a 1/8” nozzle. Overlap patterns were generated based on the different pressures throughout the field and the resulting catch-can values were combined into a global value. This value was compared against the result of the current evaluation technique, which combines the flow rate DUlq with the overlap DUlq (obtained from measured catch-can values at an average location in the field) to predict the global DUlq. Simple multiplication as suggested by ITRC and a statistical equation were used to combine the two components.The results of this study affirm that the ITRC rapid procedure for the collection of data for distribution uniformity evaluation of hand move sprinkler systems gives excellent results. Those are as good as to the ones obtained by the systematic collection of catch-can data across the field.Simple multiplication of the DUlq component values, as is done with the ITRC evaluation programs, underestimates the system DUlq. The difference between the control values and the computed ones are at most 8{\%}. The magnitude of the error is smaller (less than 4{\%}) when alternate sets are considered.The statistical equation used to combine DUlq component values underestimates the real DUlq by less than 3{\%} when the systems have regular spacing. When the spacing is reduced there is some overestimation but the errors are less than 5{\%}. These good estimations are obtained even though the actual values of Ka for the components of the distribution uniformity were different than the 1.27 value used in the computations.},
