keywords = { Crop duration, Crop water productivity, Cultivar, Date of transplanting, Irrigation, Water saving,Rice},
number = {7},
pages = {1096--1104},
title = {{Integrated effect of transplanting date, cultivar and irrigation on yield, water saving and water productivity of rice (Oryza sativa L.) in Indian Punjab: Field and simulation study}},
