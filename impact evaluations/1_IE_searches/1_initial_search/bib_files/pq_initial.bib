@article{UCAN2010227,
abstract = {Irrigation is of great importance to sesame production in Mediterranean-type environments due to positive effect on flower and capsule numbers. Water stress limits sesame growth and development. The objective of this study was to determine the effects of different irrigation programs on flower and capsule numbers, shedding percentage, and yield of sesame. The amount of water used was based on free surface evaporation from a screened class-A pan. Irrigation treatments consisted of three different irrigation intervals (I1: 7-day; I2: 14-day; I3: 21-day), four plant-pan coefficients (Kcp1: 0.60; Kcp2: 0.80; Kcp3: 1.00 and Kcp4: 1.20). The first irrigation was carried out when the available water was at 40{\%} level in the 90cm of the soil profile. The results revealed significant linear relationships among the irrigation water, water consumption and total number of flower per plant. On the other hand, the highest total number of flower per plant and the highest shedding percentage were obtained from 7-day intervals. Irrigation interval–irrigation amount interaction was significant for the number of sound capsule per plant. The highest number of sound capsule was obtained from Kcp3 treatment at I1 irrigation interval. Therefore, sesame yield increased by increasing the capsule number.},
author = {U{\c{c}}an, Kenan and Killi, Fatih},
doi = {https://doi.org/10.1016/j.agwat.2010.08.005},
issn = {0378-3774},
journal = {Agricultural Water Management},
keywords = { Capsule number, Flower number, Pan evaporation, Shedding percentage,Sesame},
number = {2},
pages = {227--233},
title = {{Effects of different irrigation programs on flower and capsule numbers and shedding percentage of sesame}},
url = {https://www.sciencedirect.com/science/article/pii/S0378377410002726},
volume = {98},
year = {2010}
}
@article{Senyigit2011,
abstract = {This study was carried out to determine the effects of different irrigation programs on yield and quality parameters of eggplant under greenhouse conditions, using Class A pan evaporation calculations and different plant-pan coefficients. Irrigation water was applied through drip irrigation method twice a week during the growing period. Irrigation treatments consisted of five plant-pan coefficients (S1: k sub(cp) = non-irrigation, S2: k sub(cp) = 0.50, S3: k sub(cp) = 0.75, S4: k sub(cp) = 1.00 and S5: k sub(cp) = 1.25). The amount of irrigation water ranged between 95.2 and 238.7 mm among the treatments. Evapotranspiration (ET) values varied from 93.1 to 466.3 mm for the treatments. The highest yield was obtained from the S3 and S4 treatments. A significant polynomial correlation was obtained between the yield and irrigation water, and between the yield and ET (P {\textless} 0.01). This indicated that when irrigation water and ET increased, yield also increased to a certain point. However, when the amount of irrigation water exceeded the plant water requirement, eggplant yield decreased. Yield response factor (K sub(y)) was determined as 0.81. Since K sub(y) {\textless} 1, eggplants were not sensitive to water deficiency. In addition, the highest water use efficiency (WUE) and irrigation water use efficiency (IWUE) values were calculated in the S3 (12.9 kgm super(-3)) and S2 (44.2 kgm super(-3)) treatments, while the lowest WUE and IWUE values were calculated in the S5 (7.9 and 15.5 kgm super(-3)) treatment to which the highest irrigation water was applied. This finding indicated that WUE and IWUE values decreased with the increasing irrigation water and ET. These results suggested that S3 (k sub(cp) = 0.75) treatment can be the most appropriate irrigation program for eggplant with higher yield and WUE under greenhouse conditions.},
address = {Department of Agricultural Structure and Irrigation, Faculty of Agriculture, Suleyman Demirel University, Isparta, 32260, Turkey, ulas@ziraat.sdu.edu.tr},
