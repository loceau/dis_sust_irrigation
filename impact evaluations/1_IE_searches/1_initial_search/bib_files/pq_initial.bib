keywords = {Southern Europe; adaptive management; agrometeorology; climate change; climate effect; environmental effect; irrigation system; stochasticity; water management; water planning},
pages = {226 -- 233},
title = {{Efficient water management policies for irrigation adaptation to climate change in Southern Europe}},
